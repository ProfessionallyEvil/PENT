Vagrant.configure("2") do |config|
  # Admin user name and password
  config.ssh.username = "SSH_USERNAME"
  VM_PUBLIC_NETWORK='10.42.100'
  config.vm.network "public_network",
    ip: "#{VM_PUBLIC_NETWORK}.PUBLIC_LAST_OCTET",
    bridge: [
      "enp4s0",
      "en0"
    ]
end
